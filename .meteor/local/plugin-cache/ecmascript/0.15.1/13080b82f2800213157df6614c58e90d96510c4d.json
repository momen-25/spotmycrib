ilters.splice(index, 1);\n  }\n};\n\nBaseErrorModel.prototype.applyFilters = function (type, message, error, subType) {\n  for (var lc = 0; lc < this._filters.length; lc++) {\n    var filter = this._filters[lc];\n\n    try {\n      var validated = filter(type, message, error, subType);\n      if (!validated) return false;\n    } catch (ex) {\n      // we need to remove this filter\n      // we may ended up in a error cycle\n      this._filters.splice(lc, 1);\n\n      throw new Error(\"an error thrown from a filter you've suplied\", ex.message);\n    }\n  }\n\n  return true;\n};","map":{"version":3,"sources":["packages/mdg:meteor-apm-agent/lib/models/base_error.js"],"names":["BaseErrorModel","options","_filters","prototype","addFilter","filter","push","Error","removeFilter","index","indexOf","splice","applyFilters","type","message","error","subType","lc","length","validated","ex"],"mappings":"AAAAA,cAAc,GAAG,UAASC,OAAT,EAAkB;AACjC,OAAKC,QAAL,GAAgB,EAAhB;AACD,CAFD;;AAIAF,cAAc,CAACG,SAAf,CAAyBC,SAAzB,GAAqC,UAASC,MAAT,EAAiB;AACpD,MAAG,OAAOA,MAAP,KAAkB,UAArB,EAAiC;AAC/B,SAAKH,QAAL,CAAcI,IAAd,CAAmBD,MAAnB;AACD,GAFD,MAEO;AACL,UAAM,IAAIE,KAAJ,CAAU,iCAAV,CAAN;AACD;AACF,CAND;;AAQAP,cAAc,CAACG,SAAf,CAAyBK,YAAzB,GAAwC,UAASH,MAAT,EAAiB;AACvD,MAAII,KAAK,GAAG,KAAKP,QAAL,CAAcQ,OAAd,CAAsBL,MAAtB,CAAZ;;AACA,MAAGI,KAAK,IAAI,CAAZ,EAAe;AACb,SAAKP,QAAL,CAAcS,MAAd,CAAqBF,KAArB,EAA4B,CAA5B;AACD;AACF,CALD;;AAOAT,cAAc,CAACG,SAAf,CAAyBS,YAAzB,GAAwC,UAASC,IAAT,EAAeC,OAAf,EAAwBC,KAAxB,EAA+BC,OAA/B,EAAwC;AAC9E,OAAI,IAAIC,EAAE,GAAC,CAAX,EAAcA,EAAE,GAAC,KAAKf,QAAL,CAAcgB,MAA/B,EAAuCD,EAAE,EAAzC,EAA6C;AAC3C,QAAIZ,MAAM,GAAG,KAAKH,QAAL,CAAce,EAAd,CAAb;;AACA,QAAI;AACF,UAAIE,SAAS,GAAGd,MAAM,CAACQ,IAAD,EAAOC,OAAP,EAAgBC,KAAhB,EAAuBC,OAAvB,CAAtB;AACA,UAAG,CAACG,SAAJ,EAAe,OAAO,KAAP;AAChB,KAHD,CAGE,OAAOC,EAAP,EAAW;AACX;AACA;AACA,WAAKlB,QAAL,CAAcS,MAAd,CAAqBM,EAArB,EAAyB,CAAzB;;AACA,YAAM,IAAIV,KAAJ,CAAU,8CAAV,EAA0Da,EAAE,CAACN,OAA7D,CAAN;AACD;AACF;;AAED,SAAO,IAAP;AACD,CAfD","sourcesContent":["BaseErrorModel