input[type=checkbox] + label:before{
  height:16px;
  margin-right: 4px;
  content: " ";
  display:inline-block;
  vertical-align: -3px;
  transition: 0.3s;
 border: 2px solid #60c06c;
  border-radius:10px;

  transition: 0.3s;
}
/* DEFAULT */
input[type=radio] + label:before{
  border-radius:50%;
  width:16px;
  margin-right: 17px;
}
input[type=checkbox] + label:before{
  border-radius:2px;
  width:26px;
}
/* CHECKED */
